[global_config]
  scroll_tabbar = True
  title_hide_sizetext = True
  title_use_system_font = False
  title_font = Sans 11
[keybindings]
[profiles]
  [[default]]
    background_color = "#300a24"
    background_darkness = 0.7
    cursor_color = "#aaaaaa"
    foreground_color = "#ffffff"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
